{
    "name": "devleaks/gip",
    "description": "Yii 2 GIP Application",
    "keywords": ["yii2", "gip"],
    "homepage": "http://www.devleaks.be/gip/",
    "type": "project",
    "license": "MIT",
    "support": {
        "source": "https://github.com/devleaks/gip"
    },
    "minimum-stability": "dev",
	"prefer-stable": true,
	"repositories": [
		{
		    "type": "vcs",
		    "url": "https://github.com/devleaks/yii2-sieve"
		},
		{
		    "type": "vcs",
		    "url": "https://github.com/fetus-hina/yii2-flot"
		},
		{
		    "type": "vcs",
		    "url": "https://github.com/devleaks/yii2-metafizzy"
		},
		{
		    "type": "vcs",
		    "url": "https://github.com/devleaks/yii2-recurinput"
		},
		{
		    "type": "vcs",
		    "url": "https://github.com/devleaks/yii2-weather"
		}
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
		"yiisoft/yii2-apidoc": "*",
        "yiisoft/yii2-jui": "~2.0@dev",
        "2amigos/yii2-chartjs-widget": "~2.0",
		"2amigos/yii2-gallery-widget": "*",
        "bburim/flot": "dev-master",
		"bower-asset/chardin.js": "*",
        "bower-asset/dynatable": "^0.3.1",
        "bower-asset/gridmanager.js": "^0.3.1",
        "bower-asset/mapbox.js": "^2.4",
        "bower-asset/weather-widget": "*",
        "brandonwamboldt/utilphp": "1.0.*",
        "dektrium/yii2-user": "*",
        "devgroup/yii2-jsoneditor": "*",
        "devleaks/yii2-sieve": "*",
        "devleaks/yii2-weather": "dev-master",
        "dmstr/yii2-adminlte-asset": "2.*",
        "e-frank/yii2-requirejs": "dev-master",
        "fedemotta/yii2-gridstack": "*",
        "fedemotta/yii2-gridster": "*",
        "insolita/yii2-iconpicker": "1.0.0",
        "insolita/yii2-migration-generator": "*",
		"kartik-v/yii2-builder": "dev-master",
		"kartik-v/yii2-date-range": "*",
		"kartik-v/yii2-datecontrol": "dev-master",
		"kartik-v/yii2-detail-view": "dev-master",
		"kartik-v/yii2-grid": "*",
		"kartik-v/yii2-icons": "dev-master",
		"kartik-v/yii2-markdown": "dev-master",
		"kartik-v/yii2-mpdf": "dev-master",
        "kartik-v/yii2-popover-x": "*",
		"kartik-v/yii2-slider": "*",
        "kartik-v/yii2-widget-colorinput": "*",
		"kartik-v/yii2-widget-datepicker": "*",
		"kartik-v/yii2-widgets": "dev-master",
        "kop/yii2-scroll-pager": "dev-master",
        "macgyer/yii2-materializecss": "*",
        "machour/yii2-sparkline": "*",
		"mdmsoft/yii2-ar-behaviors": "*",
		"miloschuman/yii2-highcharts-widget": "*",
        "mootensai/yii2-enhanced-gii": "dev-master",
        "morozovsk/websocket-examples": "^1.1",
        "morozovsk/yii2-websocket": "^1.0",
        "philippfrenzel/yii2fullcalendar": "*",
        "raoul2000/yii2-bootswatch-asset": "*",
        "romdim/yii2-bootstrap-material": "dev-master",
        "sagem-cassiopee/php-metar-decoder": "dev-master",
        "sagem-cassiopee/php-taf-decoder": "dev-master",
        "schmunk42/yii2-giiant": "*",
        "textalk/websocket": "^1.2",
        "vova07/yii2-imperavi-widget": "*",
        "warrence/yii2-kartikgii": "dev-master",
        "xfg/yii2-mustache.js": "^1.0",
        "yii2mod/yii2-chosen-select": "*",
        "yii2mod/yii2-selectize": "*",
        "yurkinx/yii2-image": "dev-master",
        "composer/composer": "~1.0@dev",
        "2amigos/yii2-leaflet-extension": "^1.1"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*",
		"jamband/yii2-schemadump": "*"
    },
    "config": {
        "process-timeout": 1800,
		"fxp-asset": {
			"installer-paths": {
	            "npm-asset-library": "vendor/npm",
	            "bower-asset-library": "vendor/bower"
	        },
			"repositories": [
		         {
		             "name": "bower-asset/eventemitter",
		             "type": "bower-vcs",
		             "url": "git://github.com/Olical/EventEmitter.git"
		         }
		     ]
		}
    }
}
